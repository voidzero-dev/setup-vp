{
  "name": "setup-vite-plus-action",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "lint": "vite lint --type-aware --type-check",
    "lint:fix": "vite run lint --fix",
    "build": "vite lib",
    "typecheck": "tsc --noEmit",
    "test": "vite test run",
    "test:watch": "vite test -w",
    "fmt": "vite fmt",
    "fmt:check": "vite fmt --check"
  },
  "dependencies": {
    "@actions/cache": "^4.0.0",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.5.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.0",
    "vite-plus": "latest"
  }
}
